generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id          Int  @id @default(autoincrement())
  email       String @unique
  password    String
  username    String
  role        String?
  desa_id     Int?
  desa        desa? @relation(fields: [desa_id],references: [id])
  pengkajian  pengkajian[] @relation("pengkajianRelation")
}
model kecamatan{
  id        Int  @id @default(autoincrement())
  nama      String
  alamat    String
  desa      desa[]
}
model desa {
  id                  Int  @id @default(autoincrement())
  nama                String
  alamat              String
  telepon             String
  website             String
  email               String
  luas                Float
  letak_dan_batas     String
  banyak_dusun        Int
  jumlah_penduduk     Int
  akses_komunikasi    String
  akses_transportasi  String
  nama_responden      String
  nip_responden       String
  jabatan_responden   String
  nomor_hp_responden  String
  kecamatan_id        Int
  kecamatan           kecamatan @relation(fields: [kecamatan_id], references: [id])
  berita              berita[]
  pelaporanAwal       pelaporanAwal[]
  pengkajian          pengkajian[]
  user                user[]
}
model berita{
  id                  Int @id@default(autoincrement())
  judul               String
  deskripsi           String @db.Text
  gambar              String
  desa_id             Int
  created_at          DateTime @default(now())
  desa                desa @relation(fields: [desa_id], references: [id])
}
model pelaporanAwal {
  id                      Int @id @default(autoincrement())
  jenisBencana            String
  waktuKejadian           DateTime
  deskripsi               String
  lokasi                  String
  desa_id                 Int
  desa                    desa @relation(fields: [desa_id], references: [id])
  jumlahPendudukTerancam  Int
  created_at              DateTime @default(now())
  korbanMeninggal         korbanMeninggal[]
  korbanHilang            korbanHilang[]
  korbanLuka              korbanLuka[]
  korbanPengungsi         korbanPengungsi[]
  aksesDanKeadaan         aksesDanKeadaan[]
  upaya                   upaya[]
}
model korbanMeninggal {
  id                      Int @id @default(autoincrement())
  nama                    String 
  alamat                  String
  jenisKelamin            String
  usia                    Int
  tempatMeninggal         String
  penyebab                String
  pelaporan_id            Int
  pelaporan               pelaporanAwal @relation(fields: [pelaporan_id], references: [id])
}
model korbanHilang {
  id                    Int @id @default(autoincrement())
  nama                  String
  alamat                String
  jenisKelamin          String
  usia                  Int
  lokasiHilang          String
  pelaporan_id          Int
  pelaporan             pelaporanAwal @relation(fields: [pelaporan_id], references: [id])
}
model korbanLuka {
  id                    Int @id @default(autoincrement())
  namaFaskes            String
  jenisRawat            String
  lakiLaki              Int
  wanita                Int
  jumlah                Int
  pelaporan_id          Int
  pelaporan             pelaporanAwal @relation(fields: [pelaporan_id], references: [id])
}
model korbanPengungsi {
  id                    Int @id @default(autoincrement())
  lokasi                String
  gangguanAnak          Int
  gangguanDewasa        Int
  lakiLaki              Int
  perempuan             Int
  jumlah                Int
  bayi                  Int
  balita                Int
  bumil                 Int
  buteki                Int
  cacatLaki             Int
  cacatPerempuan        Int
  lansiaLaki            Int
  lansiaPerempuan       Int
  pelaporan_id          Int
  pelaporan             pelaporanAwal @relation(fields: [pelaporan_id], references: [id])
}
model aksesDanKeadaan {
  id                    Int   @id @default(autoincrement())
  mudahDiakses          String
  sukar                 String
  Narahubung            String
  alamat                String
  jalurKomuikasi        String
  keadaanListrik        String
  bangunanSekolah       String
  bangunanBalai         String
  sumberAir             String
  fasilitasUmum         String
  bangunanPustu         String
  fasilitasIbadah       String
  pelaporan_id          Int
  pelaporan             pelaporanAwal @relation(fields: [pelaporan_id], references: [id])
}
model upaya {
  id                            Int @id @default(autoincrement())
  upayaPenanggulangan           String
  pelayananKesehatan            String
  pelayananKesehatanReproduksi  String
  pengendalianPenyakit          String
  DVI                           String
  pelayananGizi                 String
  logisticKesehatan             String
  pelayananJiwa                 String
  HambatanPelayananKesehatan    String
  bantuanYangDiperlukanSegera   String
  rencanaTindakLanjut           String
  statusDesa                    String
  pelaporan_id                  Int
  pelaporan                     pelaporanAwal @relation(fields: [pelaporan_id],references: [id])
  kelompokRentan                kelompokRentan[] @relation(name: "upaya_id")
}
model kelompokRentan {
  id                            Int @id @default(autoincrement())
  bayi                          Int
  bumil                         Int
  balita                        Int
  cacat                         Int
  buteki                        Int
  lansia                        Int
  upaya_id                      Int?
  upaya                         upaya? @relation(name: "upaya_id",fields: [upaya_id],references: [id])
}
model pengkajian {
  id                            Int @id @default(autoincrement())
  jenis_ancaman                 String
  riwayat                       String
  desa_id                       Int
  desa                          desa @relation(fields: [desa_id],references: [id])
  user_id                       Int?
  user                          user? @relation(name: "pengkajianRelation",fields: [user_id],references: [id]) 
  created_at                    DateTime @default(now())
  peraturan                     peraturan[]
  kapasitas                     kapasitas[]
  peringatanDini                peringatanDini[]
  mitigasi                      mitigasi[]
  kesiapSiagaan                 kesiapSiagaan[]
}
model peraturan{
  id                            Int @id @default(autoincrement())
  a                             Boolean
  b                             Boolean
  c                             Boolean
  d                             Boolean
  e                             Boolean
  f                             Boolean
  g                             Boolean
  h                             Boolean
  total                         Float
  pengkajian_id                 Int
  pengkajian                    pengkajian @relation(fields: [pengkajian_id],references: [id])
}
model kapasitas{
  id                            Int @id @default(autoincrement())
  a                             Boolean
  b                             Boolean
  c                             Boolean
  d                             Boolean
  e                             Boolean
  f                             Boolean
  g                             Boolean
  h                             Boolean
  total                         Float
  pengkajian_id                 Int
  pengkajian                    pengkajian @relation(fields: [pengkajian_id],references: [id])
}
model peringatanDini {
  id                            Int @id @default(autoincrement())
  a                             Boolean
  b                             Boolean
  total                         Float
  pengkajian_id                 Int
  pengkajian                    pengkajian @relation(fields: [pengkajian_id],references: [id])
}
model mitigasi {
  id                            Int @id @default(autoincrement())
  a                             Boolean
  b                             Boolean
  c                             Boolean
  d                             Boolean
  e                             Boolean
  f                             Boolean
  g                             Boolean
  h                             Boolean
  i                             Boolean
  j                             Boolean
  k                             Boolean
  total                         Float
  pengkajian_id                 Int
  pengkajian                    pengkajian @relation(fields: [pengkajian_id],references: [id])
}
model kesiapSiagaan{
  id                            Int @id @default(autoincrement())
  a                             Boolean
  b                             Boolean
  c                             Boolean
  d                             Boolean
  e                             Boolean
  f                             Boolean
  g                             Boolean
  h                             Boolean
  i                             Boolean
  j                             Boolean
  k                             Boolean
  l                             Boolean
  m                             Boolean
  n                             Boolean
  total                         Float
  pengkajian_id                 Int
  pengkajian                    pengkajian @relation(fields: [pengkajian_id],references: [id])
}